<p id="notice"><%= notice %></p>

<h1>Infomations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Target</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infomations.each do |infomation| %>
      <tr>
        <td><%= infomation.title %></td>
        <td><%= infomation.body %></td>
        <td><%= infomation.target %></td>
        <td><%= infomation.user_id %></td>
        <td><%= link_to 'Show', infomation %></td>
        <td><%= link_to 'Edit', edit_infomation_path(infomation) %></td>
        <td><%= link_to 'Destroy', infomation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Infomation', new_infomation_path %>
