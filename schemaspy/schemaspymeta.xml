<schemaMeta xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://schemaspy.org/xsd/6/schemameta.xsd" >
    <tables>
        <table name="users" comments="ユーザー" />
        <table name="admin_users" comments="管理者" />
        <table name="infomations" comments="お知らせ" />
        <table name="spaces" comments="スペース">
            <column name="created_user_id">
                <foreignKey table="users" column="id" />
            </column>
            <column name="last_updated_user_id">
                <foreignKey table="users" column="id" />
            </column>
        </table>
        <table name="members" comments="メンバー">
            <column name="invitationed_user_id">
                <foreignKey table="users" column="id" />
            </column>
            <column name="last_updated_user_id">
                <foreignKey table="users" column="id" />
            </column>
        </table>
        <table name="downloads" comments="ダウンロード">
            <column name="space_id">
                <foreignKey table="spaces" column="id" />
            </column>
        </table>
        <table name="download_files" comments="ダウンロードファイル" />
        <table name="invitations" comments="招待">
            <column name="created_user_id">
                <foreignKey table="users" column="id" />
            </column>
            <column name="last_updated_user_id">
                <foreignKey table="users" column="id" />
            </column>
        </table>
    </tables>
</schemaMeta>
